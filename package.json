{
  "name": "parse-query-gen",
  "version": "1.0.0",
  "description": "Helps you generate queries for your Parse Backend, structuring them with objects",
  "main": "out/index.js",
  "scripts": {
    "test": "jest --ci --silent",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublish": "tsc",
    "publish": "npm publish"
  },
  "keywords": [
    "parse",
    "parse.com",
    "query",
    "parse-query",
    "parse-server"
  ],
  "author": "Juan Camilo Guar√≠n P",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "is": "^3.2.1",
    "parse": "^1.10.1"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/out"
    ]
  }
}
